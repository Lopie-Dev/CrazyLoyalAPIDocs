            "sentiment": "positive",
            "id": "f82c6563-282e-4195-a73b-2593e4c0b606",
            "cusNumber": "+12156806297",
            "messageArray": [
              {
                "content": "Hello! I'm La Sabrosita Cantina's virtual assistant. How can I help you today?",
                "role": "assistant",
                "id": "d9463e8f-b3e1-45c6-abc2-1979c18902c6"
